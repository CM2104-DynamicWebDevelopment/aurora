<table id="diary-tbl">
    <thead>
        <tr>
            <th>Date</th>
            <th>Time</th>
            <th>Coordinates</th>
            <th>Photo</th>
        </tr>
    </thead>
    <tbody id="table-content">
    <% observation_records.forEach(function(obsr) {  %>
            <tr>
                <td><%= obsr.date %></td>
                <td><%= obsr.time %></td>
                <td><%= obsr.latitude %> , <%= obsr.longitude %></td>
                <td><img id="aurora_photo" alt="Upload Photo" src="../img/uploads/aurora/<%= obsr.observation_photo %>"></td>
            </tr>
        <% }); %>
                
   </tbody>
</table>
